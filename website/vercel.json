{
    "version": 2,
    "builds": [
        {
            "src": "website/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "150mb",
                "runtime": "python3.9.6"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "website/wsgi.py"
        }
    ]
}